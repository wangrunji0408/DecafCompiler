class Main {
	static string dayOfWeek(int n) {
		return case (n) {
			1: "Monday";
			2: "Tuesday";
			3: "Wednesday";
			4: "Thursday";
			5: "Friday";
			6: "Saturday";
			7: "Sunday";
			default: "<unknown>";
		};
	}

	static bool isOdd(int n) {
		return case (n % 2) {
			0: true;
			default: false;
		};
	}

    static void main() {
    	Print(dayOfWeek(4));
    	Print(isOdd(3));
    }
}
